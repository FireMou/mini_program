/* pages/profile/profile.wxss */

view {
    display: flex;
    /* flex-direction: column; */
    justify-content: center;
    align-items: center;
}

.version {
    position: absolute;
    bottom: 30rpx;
    font-size: 24rpx;
    color: #c9c9c9;
    width: 100%;
    text-align: center;
}

.login-wrapper {
    position: relative;
    width: 100%;
    height: 400rpx;
    /* background-color: red; */
}

.login-wrapper .login {
    color: #1e4826;
    background-color: #fff;
    border-radius: 50rpx;
    height: 100rpx;
    width: 350rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 40rpx;
}

.login-wrapper image {
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 100%;
}